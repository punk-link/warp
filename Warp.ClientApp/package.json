{
    "name": "warp-clientapp",
    "private": true,
    "version": "0.1.0",
    "type": "module",
    "packageManager": "yarn@1.22.19",
    "scripts": {
        "dev": "vite",
        "build": "vite build",
        "preview": "vite preview --port 5174",
        "typecheck": "vue-tsc --noEmit",
        "lint": "eslint .",
        "format": "prettier --write .",
        "prepare": "husky install",
        "_lint-staged-run": "lint-staged",
        "test": "vitest --run",
        "test:watch": "vitest",
        "e2e": "playwright test",
        "e2e:headed": "playwright test --headed --project=chromium",
        "e2e:install": "playwright install"
    },
    "dependencies": {
        "@fancyapps/ui": "^5.0.0",
        "@sentry/tracing": "^7.120.4",
        "@sentry/vue": "^10.23.0",
        "@tiptap/extension-link": "^3.19.0",
        "@tiptap/html": "^3.19.0",
        "@tiptap/pm": "^3.19.0",
        "@tiptap/starter-kit": "^3.19.0",
        "@tiptap/vue-3": "^3.19.0",
        "@types/dompurify": "^3.2.0",
        "dompurify": "^3.3.1",
        "vue": "^3.4.0",
        "vue-i18n": "^11.1.12",
        "vue-router": "^4.3.0"
    },
    "devDependencies": {
        "@playwright/test": "^1.56.1",
        "@tailwindcss/postcss": "^4.0.7",
        "@testing-library/vue": "^8.1.0",
        "@types/node": "^22.5.0",
        "@typescript-eslint/eslint-plugin": "^8.0.0",
        "@typescript-eslint/parser": "^8.0.0",
        "@vitejs/plugin-vue": "^5.0.0",
        "autoprefixer": "^10.4.20",
        "eslint": "^9.21.0",
        "eslint-plugin-vue": "^9.27.0",
        "husky": "^9.0.0",
        "jsdom": "^25.0.0",
        "lint-staged": "^15.2.0",
        "postcss": "^8.5.3",
        "prettier": "^3.3.0",
        "tailwindcss": "^4.0.7",
        "typescript": "^5.5.0",
        "vite": "^6.0.0",
        "vitest": "^2.0.0",
        "vue-tsc": "^2.0.0"
    },
    "lint-staged": {
        "*.{js,ts,vue,json,css,md}": [
            "prettier --write",
            "eslint --fix"
        ]
    }
}
