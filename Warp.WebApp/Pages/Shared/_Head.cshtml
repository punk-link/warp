@inject IConfiguration Configuration

<link rel="icon" type="image/x-icon" href="@Url.Content("~/favicon.ico")">
<link rel="preconnect" href="https://fonts.googleapis.com">
<link rel="preconnect" href="https://fonts.gstatic.com" crossorigin>
<link rel="stylesheet" href="https://fonts.googleapis.com/css2?family=Montserrat:ital,wght@0,100..900;1,100..900&display=swap">
<link rel="stylesheet" href="https://fonts.googleapis.com/css?family=Krona%20One">
<link rel="stylesheet" href="~/fonts/icofont/icofont.min.css">
<link rel="stylesheet" href="~/css/tailwind.min.css" asp-append-version="true">
<link rel="stylesheet" href="~/dist/fancybox.css" asp-append-version="true">

<script>
    window.appConfig = {
        analytics: {
            gTag: '@Configuration["AnalyticsOptions:GoogleGTag"]',
            yandexMetrikaNumber: '@Configuration["AnalyticsOptions:YandexMetrikaNumber"]'
        },
        environment: '@Configuration["Environment"]',
        sentryDsn: '@Configuration["Sentry:FrontendDsn"]'
    };
</script>